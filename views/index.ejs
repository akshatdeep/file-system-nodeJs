<!DOCTYPE html>
<html>
  <head>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="main">
      <div class="left">
        <form action="/create" method="post">
          <input type="text" placeholder="Enter file name" name="filename">
        </form>
        <ul>
          <% filename.forEach(file => {%>
            <li>
              <a href="/file/<%= file %>"><%= file %></a>
            </li>
         <% }); %>
        </ul>
      </div>
      <div class="right"></div>
    </div>
  </body>
</html>
